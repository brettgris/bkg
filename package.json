    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "orbit-controls-es6": "^1.0.10",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-draggable": "^2.2.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.0.10",
    "react-scroll": "^1.7.6",
    "react-svg": "^2.2.10",
    "react-waypoint": "^8.0.0",
